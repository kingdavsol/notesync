{
  "expo": {
    "name": "NoteSync",
    "slug": "notesync",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "owner": "markd98",
    "extra": {
      "eas": {
        "projectId": "b30e6bd3-4217-4a85-b13f-628721a8462b"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.gg9.notesync"
    },
    "android": {
      "package": "app.gg9.notesync",
      "permissions": [
        "RECORD_AUDIO",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-dev-client",
      "expo-updates"
    ]
  }
}
