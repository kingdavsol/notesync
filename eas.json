{
  "cli": {
    "version": ">= 5.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "credentialsSource": "local",
      "platform": "android",
      "node": "20.11.1",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "credentialsSource": "local",
      "platform": "android",
      "node": "20.11.1",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "credentialsSource": "local",
      "platform": "android",
      "node": "20.11.1",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
